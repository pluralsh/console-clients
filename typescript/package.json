{
  "name": "@pluralsh/console-client",
  "version": "0.1.0",
  "description": "TypeScript REST client for Plural Console API, generated from OpenAPI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "openapi-ts",
    "build": "tsc",
    "prepublishOnly": "yarn generate && yarn build"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.91.0",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=20"
  }
}
